[tool.poetry]
name = "@pname@"
version = "@version@"
description = "@desc@"
authors = [ @authors@ ]
readme = "README.md"
homepage = "@homepage@"
packages = [{include = "@package@"}]

[tool.poetry.dependencies]
dbus-next = "^@dbusNextVersion@"
grpcio = "^@grpcioVersion@"
python = "^@pythonMajorMinorVersion@"
pythoneda-shared-pythoneda-domain = "^@pythonedaSharedPythonedaDomainVersion@"
requests = "^@requestsVersion@"

[build-system]
requires = ["poetry-core"]
build-backend = "poetry.core.masonry.api"
